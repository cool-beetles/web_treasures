%div.container
  %h3 Log in!
  %div.jumbotron
    = form_for :session, url: { action: 'create', html: { class: 'form-horizontal' }} do |f|
      %div.form-group
        = f.label :email
        = f.email_field :email
      %div.form-group
        = f.label :password
        = f.password_field :password_digest
      = f.submit "Log-in"
